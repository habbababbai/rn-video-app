{
    "expo": {
        "name": "Youtube Learn",
        "slug": "youtube-learn",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/logo/app-icon.png",
        "scheme": "youtube-learn",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.anonymous.youtube-learn"
        },
        "android": {
            "adaptiveIcon": {
                "backgroundColor": "#FFFFFF",
                "foregroundImage": "./assets/images/logo/app-icon.png"
            },
            "icon": "./assets/images/logo/app-icon.png",
            "notification": {
                "icon": "./assets/images/logo/app-icon.png",
                "color": "#8D99AE"
            },
            "edgeToEdgeEnabled": true,
            "predictiveBackGestureEnabled": false,
            "package": "com.anonymous.youtubelearn",
            "permissions": [
                "android.permission.RECEIVE_BOOT_COMPLETED",
                "android.permission.VIBRATE",
                "android.permission.WAKE_LOCK"
            ]
        },
        "web": {
            "output": "static",
            "favicon": "./assets/images/logo/app-icon.png"
        },
        "plugins": [
            "expo-router",
            "expo-dev-client",
            [
                "expo-notifications",
                {
                    "icon": "./assets/images/logo/app-icon.png",
                    "color": "#8D99AE",
                    "defaultChannel": "default"
                }
            ],
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/logo/logo.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#8D99AE"
                }
            ],
            [
                "react-native-video",
                {
                    "android": {
                        "enableBackgroundPlayback": true
                    },
                    "ios": {
                        "enableBackgroundPlayback": true
                    }
                }
            ],
            [
                "react-native-google-cast",
                {
                    "iosReceiverAppId": "CC1AD845",
                    "androidReceiverAppId": "CC1AD845",
                    "androidPlayServicesCastFrameworkVersion": "+",
                    "iosStartDiscoveryAfterFirstTapOnCastButton": false
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true,
            "reactCompiler": true
        }
    }
}
