{
    "expo": {
        "name": "Youtube Learn",
        "slug": "youtube-learn",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/logo/app-icon.png",
        "scheme": "youtube-learn",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.anonymous.youtube-learn"
        },
        "android": {
            "adaptiveIcon": {
                "backgroundColor": "#8D99AE",
                "foregroundImage": "./assets/images/logo/app-icon.png"
            },
            "notification": {
                "icon": "./assets/images/logo/app-icon.png",
                "color": "#8D99AE"
            },
            "edgeToEdgeEnabled": true,
            "predictiveBackGestureEnabled": false,
            "permissions": [],
            "package": "com.anonymous.youtubelearn"
        },
        "web": {
            "output": "static",
            "favicon": "./assets/images/logo/app-icon.png"
        },
        "plugins": [
            "expo-router",
            "expo-dev-client",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/logo/logo.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#8D99AE"
                }
            ],
            [
                "react-native-video",
                {
                    "android": {
                        "enableBackgroundPlayback": true
                    },
                    "ios": {
                        "enableBackgroundPlayback": true
                    }
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true,
            "reactCompiler": true
        }
    }
}
